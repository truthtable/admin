{
     "compilerOptions": {
          "module": "ESNext",
          "moduleResolution": "Bundler",
          "target": "ES2020",
          "jsx": "react",
          "checkJs":true,
          "allowImportingTsExtensions": true,
          "strictNullChecks": true,
          "strictFunctionTypes": true
     },
     "exclude": [
          "node_modules",
          "**/node_modules/*"
     ]
}